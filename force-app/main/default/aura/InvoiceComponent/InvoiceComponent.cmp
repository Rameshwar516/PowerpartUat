                                        <th class=" slds-cell-shrink slds-text-align_center padding" scope="col"  >
                                            <div class="slds-truncate slds-cell-fixed slds-cell-wrap" title="Sales Price" style="text-align:center ; padding-left: 2%;"><span style = "color:red;" >*</span>Sales Price</div>
                                        </th>
                                        <th class=" slds-cell-shrink slds-text-align_center padding" scope="col"  >
                                            <div class="slds-truncate slds-cell-fixed slds-cell-wrap" title="Total Price" style="text-align:center ; padding-left: 2%;">Total <br/>Price</div>
                                        </th>
                                        <th class=" slds-cell-shrink slds-text-align_center padding" scope="col"  >
                                            <div class="slds-truncate slds-cell-fixed slds-cell-wrap" title="Discount %" style="text-align:center ; padding-left: 2%;">Discount % </div>
                                        </th>
                                        <th class=" slds-cell-shrink slds-text-align_center padding" scope="col"  >
                                            <div class="slds-truncate slds-cell-fixed slds-cell-wrap" title="Discount Amount" style="text-align:center ; padding-left: 2%;">Discount<br/> Amount</div>
                                        </th>
                                        <th class="slds-cell-shrink slds-text-align_center padding" scope="col"  >
                                            <div class="slds-truncate slds-cell-fixed slds-cell-wrap" title="Total Amount" style="text-align:center ; padding-left: 2%;">Total <br/>Amount</div>
                                        </th>
                                        <th class=" slds-cell-shrink slds-text-align_center padding" scope="col"  >
                                            <div class="slds-truncate slds-cell-fixed slds-cell-wrap" title="HSN Code" style="text-align:center ; padding-left: 2%;">HSN Code</div>
                                        </th>
                                        <aura:if isTrue="{!!v.wrapMain.isExport}">
                                            <!-- Check CGST and SGST  -->
                                            <aura:if isTrue="{!!v.wrapMain.igstvalue}">
                                                <th class=" slds-cell-shrink slds-text-align_center padding" scope="col"  >
                                                    <div class="slds-truncate slds-cell-fixed slds-cell-wrap" title="CGST%" style="text-align:center; padding-left: 2%;">CGST%</div>
                                                </th>
                                                <th class=" slds-cell-shrink slds-text-align_center padding" scope="col"  >
                                                    <div class="slds-truncate slds-cell-fixed slds-cell-wrap" title="CGST Amount" style="text-align:center ; padding-left: 2%;">CGST <br/>Amount</div>
                                                </th>
                                                <th class=" slds-cell-shrink slds-text-align_center padding" scope="col"  >
                                                    <div class="slds-truncate slds-cell-fixed slds-cell-wrap" title="SGST%" style="text-align:center ; padding-left: 2%;">SGST%</div>
                                                </th>
                                                <th class=" slds-cell-shrink slds-text-align_center padding" scope="col"  >
                                                    <div class="slds-truncate slds-cell-fixed slds-cell-wrap" title="SGST Amount" style="text-align:center ; padding-left: 2%;">SGST<br/> Amount</div>
                                                </th>
                                            </aura:if>
