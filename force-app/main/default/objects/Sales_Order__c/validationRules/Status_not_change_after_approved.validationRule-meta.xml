<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status_not_change_after_approved</fullName>
    <active>true</active>
    <errorConditionFormula>ISPICKVAL(PRIORVALUE( Status__c ),&quot;Approved&quot;)&amp;&amp; ISPICKVAL( Status__c ,&quot;Draft&quot;)</errorConditionFormula>
    <errorMessage>Sales order is approved will not set the draft.</errorMessage>
</ValidationRule>
