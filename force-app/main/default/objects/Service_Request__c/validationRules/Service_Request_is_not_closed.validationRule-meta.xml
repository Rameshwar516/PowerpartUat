<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Service_Request_is_not_closed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Is_Warranty_Claim__c  , NOT(ISPICKVAL( Status__c , &apos;Closed&apos;)) )</errorConditionFormula>
    <errorDisplayField>Is_Warranty_Claim__c</errorDisplayField>
    <errorMessage>Service Request is Not Closed.</errorMessage>
</ValidationRule>
