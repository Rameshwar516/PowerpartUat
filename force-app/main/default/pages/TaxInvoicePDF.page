			<style type="text/css">
            @page 
            {  
            size:12.7in 17in; 
            margin-top: 5.00cm;
            page-break-after:always;
            @top-center 
            {                   
            content: element(header);               
            }
            @bottom-center
            {
            content: element(footer); 
            }
            }  
            div.header 
            {                
            padding: 5px; 
            padding-top:10px;
            padding-bottom:5px;
            position: running(header); 
            content: "Page " counter(page) " of " counter(pages);
            } 
            div.footer 
            {                
            //padding: 5px;               
            position: running(footer);    
            }
            @page 
            {
            @bottom-center 
            {
            padding-top: 10px;
            content: "Registered Office : N-122, Panchsheel Park, New Delhi - 100 017";
            }
            @bottom-right 
            {
            padding-top: 10px;
            content: "Page " counter(page) " of " counter(pages);
            }
            }
            
            table 
            {
            border-spacing: 0px;
            font-size:12px;
            font-family: Arial;
            -fs-table-paginate: paginate; 	
            }
            
            td {
            border-width: 1px;
            padding: 4px;
            border-style: solid;
            border-color: #000000;
            }
            
            th { 
            border-width: 1px ;
            padding: 4px ;
            border-style: solid ;
            border-color: #000000;
            }
            ol.d{
            list-style-type: lower-alpha;
            }
            <apex:outputPanel layout="none" rendered="{!IF(Invoice__c.Status__c == 'Cancelled', true, false)}">
		.watermark {
		
		background-repeat: no-repeat;
		background-position: Top;
		background-image: url("{!URLFOR($Resource.Canceled)}");
		z-index:9999;
		}
	</apex:outputPanel>
        </style>
           
